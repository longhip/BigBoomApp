doctype html
html(class="no-js", lang="")
  head
    meta(charset="utf-8")
    meta(http-equiv="x-ua-compatible", content="ie=edge")
    title= title
    meta(name="description", description=description)
    meta(name="viewport", content="width=device-width, initial-scale=1")
    link(rel="apple-touch-icon", href="apple-touch-icon.png")
    link(rel='stylesheet', href='https://fonts.googleapis.com/css?family=Roboto')
    link(rel='stylesheet', href='assets/css/icons/icomoon/styles.css')
    link(rel='stylesheet', href='assets/css/bootstrap.css')
    link(rel='stylesheet', href='assets/css/core.css')
    link(rel='stylesheet', href='assets/css/colors.css')
    link(rel='stylesheet', href='assets/css/components.css')
    link(rel='stylesheet', href='assets/css/custom.css')
    style#css!= css
  body
    #app!= body
    script(src=entry)
    script(src='assets/js/core/libraries/jquery.min.js')
    script(src='assets/js/core/libraries/bootstrap.min.js')
    script(src='assets/js/plugins/loaders/blockui.min.js')
    script(src='assets/js/plugins/ui/nicescroll.min.js')
    script(src='assets/js/core/app.js')
    script.
      window.ga=function(){ga.q.push(arguments)};ga.q=[];ga.l=+new Date;
      ga('create','#{trackingId}','auto');ga('send','pageview')
    if trackingId
      script(src="https://www.google-analytics.com/analytics.js", async=true, defer=true)
